      slidesToShow: 1,
      slidesToScroll: 1,
      arrows: false,
      fade: true,
    infinite: false,
      asNavFor: '#product .product-slider-nav',
      responsive: [
          {
            breakpoint: 768,
            settings: {
              dots: true,
            }
          }
        ]
